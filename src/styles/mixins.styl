.ant-btn
  display flex
  height fit-content
  font-weight 600
  background: transparent;
  border-radius: 8px;
  border 1px solid #27333F
  padding 9px 16px
  color $green
  font-size 12px
  align-items: center
  &:hover
    background-color $green
    color $window-dark
    border 1px solid transparent
  &:active, &:focus
    background-color transparent
    color $green
    border-color: $green
a.ant-btn
  line-height 18px
  height 40px
  padding 11px 16px !important